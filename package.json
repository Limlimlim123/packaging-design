{
  "name": "packaging-design",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && eslint . --ext .ts,.tsx",
    "test": "npm run test:unit && npm run test:e2e",
    "test:unit": "NODE_OPTIONS='--max-old-space-size=4096 --trace-warnings --unhandled-rejections=strict' vitest run --config vitest.config.ts --pool=threads --poolOptions.threads.singleThread",
    "test:e2e": "playwright test",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:watch": "vitest",
    "test:debug": "playwright test --debug",
    "test:report": "playwright show-report",
    "test:install": "playwright install chromium firefox",
    "test:codegen": "playwright codegen",
    "pretest:e2e": "npm run build",
    "test:ci": "npm run test:unit -- --run && npm run test:e2e",
    "deploy:check": "ts-node scripts/check-env.ts && ts-node scripts/check-storage.ts",
    "deploy:prepare": "npm run build && npm run db:migrate",
    "deploy:prod": "docker-compose up -d",
    "db:migrate": "ts-node scripts/db-migrate.ts",
    "db:seed": "prisma db seed",
    "db:backup": "ts-node scripts/db-backup.ts",
    "type-check": "tsc --noEmit"
  },

  "dependencies": {
    "@alicloud/dysmsapi20170525": "^3.1.1",
